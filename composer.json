{
    "name": "unclemimo/wp-composer-startingpack",
    "description": "Installing WordPress in a subdirectory with Composer (unclemimo method).",
    "version": "1.0",
    "keywords": ["wpstartingpack","wordpress","deployment"],
    "homepage": "http://atiasweb.com",
    "license": "MIT",
    "authors": [
        {
            "name": "David Atias",
            "email": "david@atias.com.ve",
            "homepage": "http://www.atiasweb.com",
            "role": "Director"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "*",
        "johnpbloch/wordpress": "4.2.*",
        "wp-sync-db/wp-sync-db":"dev-master"
    },
    "extra": {
    	"wordpress-install-dir": "wordpressfold",
        "installer-paths": {
        "wordpressfold/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
        "wordpressfold/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
        "wordpressfold/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    	}
    }
}