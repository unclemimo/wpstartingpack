{
    "name": "unclemimo/wp-composer-startingpack",
    "description": "Installing WordPress in a subdirectory with Composer (unclemimo method).",
    "version": "1.0",
    "keywords": ["wpstartingpack","wordpress","deployment"],
    "homepage": "http://atiasweb.com",
    "license": "MIT",
    "authors": [
        {
            "name": "David Atias",
            "email": "david@atias.com.ve",
            "homepage": "http://www.atiasweb.com",
            "role": "Director"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "*",
        "johnpbloch/wordpress": "4.2.*",
        "wp-sync-db/wp-sync-db":"dev-master"
    },
    "extra": {
        "installer-paths": {
            "wp/wp-content/mu-plugins/{}/": "[type:wordpress-muplugin]",
            "wp/wp-content/plugins/{}/": "[type:wordpress-plugin]",
            "wp/wp-content/themes/{}/": "[type:wordpress-theme]"
        },
        "wordpress-install-dir": "wp"
    }

}
